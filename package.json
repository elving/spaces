  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build:server",
    "setup": "rm -rf node_modules && npm cache clear --force && npm i",
    "start": "NODE_ENV=production node ./build/server.js",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node ./app/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "clean:server": "cd ./build && rm -rf *",
    "build:server": "NODE_ENV=production npm run clean:server && ./node_modules/.bin/babel --copy-files --out-dir ./build ./app --source-maps --minified",
    "deploy": "git push dokku master",
    "task:backup-db": "sh ./tasks/backup-db.sh",
    "task:deploy-static": "npm run build && sh ./tasks/deploy-static.sh",
    "task:sync-local-db": "sh ./tasks/sync-local-db.sh",
    "task:copy-cron-tasks": "scp -r ./cron root@joinspaces.co:/cron",
    "task:update-instace": "sh ./tasks/update-instance.sh"
  },
